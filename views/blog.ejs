    <section class="blog-area section">
        <div class="container">

            <div class="row">

                <% blog.forEach(function(blog) { %>
                <div class="col-lg-4 col-md-6">
                    <div class="card h-100">
                        <div class="single-post post-style-1">

                            <div class="blog-image"><img src="images/im.png" alt="Blog Image"></div>

                            <a class="avatar" href="#"><img src="images/girl.png" alt="Profile Image"></a>

                            <div class="blog-info">

                                <h4 class="title"><a href="/blog/<%= blog._id%>"><b><%= blog.title%>
                                        </b></a></h4>

                                <ul class="post-footer">
                                    <!-- <li><a href="#"><i class="button"></i>Read More</a></li> -->
                                    <li><a href="/blog/<%= blog._id%>"><i class="Read More"></i>Read More</a></li>
                                </ul>

                            </div><!-- blog-info -->
                        </div><!-- single-post -->
                    </div><!-- card -->
                </div><!-- col-lg-4 col-md-6 -->

                <% }) %>


            </div><!-- row -->

            <a class="load-more-btn" href="#"><b>Load More</b></a>

        </div><!-- container -->
    </section><!-- section -->

